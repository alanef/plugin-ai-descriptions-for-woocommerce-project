{
  "name": "fullworks/ai-descriptions-for-woocommerce",
  "description": "Wordpress plugin",
  "type": "wordpress-plugin",
  "license": "GPL",
  "authors": [
    {
      "name": "alan",
      "email": "alan@fullworks.net"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "vendor-dir": "vendor",
